{
    "type": "ThemeStyle",
    "light": "#ffffff",
    "onLight": "#050806",
    "dark": "#000000",
    "onDark": "#FBFFF2",
    "primary": "#9F5AFF",
    "onPrimary": "#FFFFFF",
    "secondary": "#A0FFFF",
    "onSecondary": "#050806",
    "complementary": "#565862",
    "onComplementary": "#FBFFF2",
    "fontBody": "Raleway",
    "fontHeading": "Orbitron",
    "textShadow": {
    "glow": "0px 0px 10px #9F5AFF",
    "cyan": "0px 0px 8px #A0FFFF"
    },
    "h1": {
        "size": "7xl",
        "weight": "bold",
        "decoration": "none",
        "case": "uppercase",
        "letterSpacing": "wide"
    },
    "h2": {
        "size": "6xl",
        "weight": "semibold",
        "decoration": "none",
        "case": "uppercase",
        "letterSpacing": "wide"
    },
    "h3": {
        "size": "5xl",
        "weight": "medium",
        "decoration": "none",
        "case": "uppercase",
        "letterSpacing": "normal"
    },
    "h4": {
        "size": "3xl",
        "weight": "medium",
        "decoration": "none",
        "case": "uppercase",
        "letterSpacing": "normal"
    },
    "h5": {
        "size": "xl",
        "weight": "medium",
        "decoration": "none",
        "case": "uppercase",
        "letterSpacing": "normal"
    },
    "h6": {
        "size": "lg",
        "weight": "medium",
        "decoration": "none",
        "case": "uppercase",
        "letterSpacing": "normal"
    },
    "buttonPrimary": {
        "weight": "normal",
        "case": "none",
        "letterSpacing": "normal",
        "horizontalPadding": 20,
        "verticalPadding": 15
    },
    "buttonSecondary": {
        "weight": "normal",
        "case": "none",
        "letterSpacing": "normal",
        "horizontalPadding": 20,
        "verticalPadding": 15
    },
    "link": {
        "weight": "normal",
        "case": "uppercase",
        "letterSpacing": "normal"
    }
}
